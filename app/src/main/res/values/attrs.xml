<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FixedWidthLayout">
        <attr name="aspect_ratio" format="float" />
    </declare-styleable>
    <declare-styleable name="FixedImageView">
        <attr name="width_height_ratio" format="float" />
    </declare-styleable>
    <declare-styleable name="SortTabView">
        <attr name="sort_title" format="string" />
        <attr name="sort_property" format="string" />
    </declare-styleable>
    <declare-styleable name="CommonBanner">
        <attr name="banner_type" format="enum">
            <!--主界面导航-->
            <enum name="type_main" value="10" />
            <enum name="type_one" value="1" />
            <enum name="type_two" value="2" />
            <enum name="type_three" value="3" />
            <enum name="type_four" value="4" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FixedHeightLayout">
        <attr name="fixed_height_aspect_ratio" format="float" />
    </declare-styleable>
</resources>